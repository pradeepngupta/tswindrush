{
  "name": "tswindrush",
  "homepage": "https://tsw-ind-rush.vercel.app/",
  "author": "Pradeep Gupta <pradeepngupta@gmail.com>",
  "version": "1.0.0",
  "private": true,
  "description": "A Next.js project template with TypeScript and Tailwind CSS for rapid development.",
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeepngupta/tswindrush.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "npx tsc --noEmit",
    "lint": "eslint --fix .",
    "format": "prettier --check . --config ./.prettierrc",
    "format:fix": "prettier --write . --config ./.prettierrc",
    "prepare": "husky",
    "pretest": "npm run lint && tsc --noEmit",
    "posttest": "npm run format",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --",
    "test:e2e": "npx playwright test",
    "test:e2e-ui": "npx playwright test --ui",
    "test:e2e-debug": "npx playwright test --debug"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "16.1.1",
    "next": "16.1.1",
    "npm": "11.7.0",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "3.3.3",
    "@eslint/js": "9.39.2",
    "@playwright/test": "1.57.0",
    "@swc/jest": "0.2.39",
    "@tailwindcss/postcss": "4.1.18",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.1",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/lint-staged": "14.0.0",
    "@types/node": "25.0.3",
    "@types/react": "19.2.9",
    "@typescript-eslint/eslint-plugin": "8.51.0",
    "@typescript-eslint/parser": "8.53.1",
    "autoprefixer": "10.4.23",
    "eslint": "9.39.2",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "husky": "9.1.7",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "lint-staged": "16.2.7",
    "postcss": "8.5.6",
    "prettier": "3.7.4",
    "tailwindcss": "4.1.18",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20"
