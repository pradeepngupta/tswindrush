{
  "name": "tswindrush",
  "homepage": "https://tsw-ind-rush.vercel.app/",
  "author": "Pradeep Gupta <pradeepngupta@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/pradeepngupta/tswindrush.git"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "tsc": "npx tsc --noEmit",
    "lint": "next lint --fix",
    "format": "prettier --check . --config ./.prettierrc",
    "format:fix": "prettier --write . --config ./.prettierrc",
    "prepare": "husky",
    "pretest": "npm run lint && tsc --noEmit",
    "posttest": "npm run format",
    "test:jest": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test": "npm run test:jest --",
    "test:e2e": "npx playwright test",
    "test:e2e-ui": "npx playwright test --ui",
    "test:e2e-debug": "npx playwright test --debug"
  },
  "dependencies": {
    "next": "15.4.1",
    "npm": "11.4.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@playwright/test": "1.54.1",
    "@swc/jest": "0.2.39",
    "@tailwindcss/postcss": "4.1.11",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/lint-staged": "13.3.0",
    "@types/node": "24.0.13",
    "@types/react": "19.1.8",
    "@typescript-eslint/eslint-plugin": "8.36.0",
    "@typescript-eslint/parser": "8.36.0",
    "autoprefixer": "10.4.21",
    "eslint": "9.31.0",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "5.5.1",
    "husky": "9.1.7",
    "jest": "30.0.4",
    "jest-environment-jsdom": "30.0.4",
    "lint-staged": "16.1.2",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.11",
    "tailwindcss-animate": "1.0.7",
    "ts-jest": "29.4.0",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": ">=20"
